{
  "name": "private-portfolio",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && node scripts/add-pwa-tags.js dist",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|native-base|react-native-svg|tamagui|@tamagui/.*)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/react-native/matchers"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@tamagui/config": "^1.144.3",
    "@tamagui/core": "^1.144.3",
    "@tanstack/query-async-storage-persister": "^5.90.22",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-persist-client": "^5.90.22",
    "@types/uuid": "^10.0.0",
    "date-fns": "^4.1.0",
    "expo": "~54.0.32",
    "expo-camera": "^17.0.10",
    "expo-constants": "^18.0.13",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "^19.0.21",
    "expo-linking": "^8.0.11",
    "expo-router": "^6.0.22",
    "expo-sharing": "^14.0.8",
    "expo-sqlite": "^16.0.10",
    "expo-status-bar": "~3.0.9",
    "jsqr": "^1.4.0",
    "pako": "^2.1.0",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-svg": "^15.15.1",
    "react-native-web": "^0.21.2",
    "tamagui": "^1.144.3",
    "uuid": "^13.0.0",
    "zustand": "^5.0.10"
